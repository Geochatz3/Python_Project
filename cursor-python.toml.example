# Example configuration file for cursor-python
# Copy this to `cursor-python.toml` or `cursor_python.toml` in your project root

[cursor_python]
# Number of random circles to generate
count = 10

# Radius range for generated circles
min_radius = 1.0
max_radius = 10.0

# Random seed for reproducible runs (optional)
# seed = 42

# Logging configuration
log_level = "INFO"  # DEBUG, INFO, WARNING, ERROR, CRITICAL
log_format = "text"  # text or json

# Output format
output_format = "text"  # text or json

